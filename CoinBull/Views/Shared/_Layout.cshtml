<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="~/Images/bullicon.png">
    <title>CoinBull</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="myNav">
        <a href="~/Home/Index" style="float:left;padding-left:5px;"><img src="~/Images/bullicon.png" style=" max-width: 40px;" /></a>
        <span class="navLogin">@Html.Partial("_LoginPartial")</span>
        
    </div>

    <div class="container body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
